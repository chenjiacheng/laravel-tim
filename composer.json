{
    "name": "chenjiacheng/laravel-tim",
    "description": "腾讯云 即时通信 IM SDK for Laravel",
    "type": "library",
    "keywords": [
        "php",
        "im",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "chenjiacheng",
            "email": "405339556@qq.com"
        }
    ],
    "require": {
        "php": ">=8.0.2",
